{
  "name": "track-dat-baby-api",
  "version": "1.0.0",
  "description": "Node/Hapi API for Track Dat Baby",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "gulp",
    "test": "jest",
    "watch": "nodemon --watch dist dist/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "author": "Wes Cole <wesley.cole87@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "boom": "^7.2.0",
    "good": "^8.1.0",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^17.2.3",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "mongoose": "^5.0.10",
    "node": "^9.8.0",
    "vision": "^5.3.2"
  },
  "devDependencies": {
    "@types/boom": "^7.2.0",
    "@types/hapi": "^17.0.7",
    "@types/jest": "^22.2.0",
    "@types/mongoose": "^5.0.7",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.5",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.1",
    "jest": "^22.4.2",
    "nodemon": "^1.17.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
